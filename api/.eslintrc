{
    "extends": "airbnb-base/legacy",
    "env": {
      "es6": true,
      "commonjs": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules": {
      "func-names": [
        "error",
        "never"
      ],
      "no-mixed-operators": 0,
      "no-param-reassign": 0,
      "no-new": 0,
      "indent": [
        "error",
        "tab"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "no-tabs": "off",
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "class-methods-use-this": "off"
    }

}
